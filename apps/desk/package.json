{
  "name": "@fishprovider/desk",
  "productName": "FishProvider",
  "description": "FishProvider",
  "author": "FishProvider",
  "version": "5.0.0",
  "main": "packages/main/dist/index.cjs",
  "type": "module",
  "scripts": {
    "lint": "eslint --cache --fix --ignore-pattern vite.config.js .",
    "type-check:main": "tsc -p packages/main/tsconfig.json",
    "type-check:preload": "tsc -p packages/preload/tsconfig.json",
    "type-check:renderer": "tsc -p packages/renderer/tsconfig.json",
    "type-check": "npm run type-check:main && npm run type-check:preload && npm run type-check:renderer",
    "update-vendors": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.js",
    "prebuild": "npm run update-vendors",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "package": "electron-builder build --config .electron-builder.config.cjs --dir",
    "predev": "npm run update-vendors",
    "dev": "cross-env NODE_ENV=development node scripts/watch.js"
  },
  "dependencies": {
    "@fishprovider/cross": "*",
    "electron-updater": "6.1.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.3",
    "cross-env": "^7.0.3",
    "electron": "25.3.0",
    "electron-builder": "24.4.0",
    "happy-dom": "10.2.0",
    "unplugin-auto-expose": "0.0.5",
    "vite": "4.4.3"
  }
}