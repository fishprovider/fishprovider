{
  "name": "@fishprovider/back",
  "version": "5.0.3",
  "type": "module",
  "scripts": {
    "lint": "eslint --cache --fix .",
    "type-check": "tsc --noEmit",
    "test": "npm run test-unit",
    "test-unit": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test-int": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules doppler run --print-config -- npx jest --runInBand",
    "clean-build": "rm -rf dist && mkdir dist",
    "prebuild": "./build-packages.sh",
    "build": "node esbuild.cjs",
    "postbuild": "./build-check.sh",
    "build-watch": "cross-env WATCH_MODE=true node esbuild.cjs",
    "start": "doppler run --print-config -- node --experimental-specifier-resolution=node dist/index.js",
    "start-watch": "doppler run --print-config -- nodemon --experimental-specifier-resolution=node dist/index.js",
    "predev": "npm run prebuild",
    "dev": "cross-env NODE_ENV=development concurrently -k 'npm run build-watch' 'npm run start-watch'"
  },
  "dependencies": {
    "@fishprovider/adapter-backend": "*",
    "@fishprovider/application-rules": "*",
    "@fishprovider/coin": "*",
    "@fishprovider/core": "*",
    "@fishprovider/enterprise-rules": "*",
    "@fishprovider/framework-mongo": "*",
    "@fishprovider/framework-redis": "*",
    "@fishprovider/swap": "*",
    "glob": "^9.3.5",
    "md5": "^2.3.0",
    "notion-client": "^6.16.0",
    "openai": "^3.3.0"
  }
}