{
  "cli": {
    "version": ">= 3.15.0"
  },
  "build": {
    "development": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal"
    },
    "development-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "channel": "staging",
      "distribution": "internal",
      "android": {
        "buildType": "app-bundle"
      }
    },
    "preview-simulator": {
      "extends": "preview",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal"
      },
      "ios": {
        "appleId": "maidh91@gmail.com",
        "ascAppId": "1619961288",
        "appleTeamId": "52DUXY3FWY"
      }
    }
  }
}
