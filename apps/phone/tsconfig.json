{
  "extends": "@tsconfig/react-native/tsconfig.json",
  // "extends": [
  //   "../../tsconfig.json",
  //   "@tsconfig/react-native/tsconfig.json",
  // ],
  "compilerOptions": {
    "exactOptionalPropertyTypes": false,
    "noPropertyAccessFromIndexSignature": false,
    "baseUrl": ".",
    "paths": {
      "~*": ["./*"]
    },
    "rootDir": ".",
    "outDir": "dist"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
  ],
  "exclude": [
    "**/node_modules",
    "**/dist",
    "**/build",
    "**/tmp",
    ".expo",
  ]
}
