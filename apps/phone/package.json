{
  "name": "phone",
  "version": "5.0.0",
  "scripts": {
    "install-packages": "cd ../.. && npm install -w packages/utils -w packages/cross",
    "build-packages": "cd ../.. && npm run build -w packages/utils -w packages/cross",
    "preinstall": "npm run install-packages && npm run build-packages",
    "doctor": "npx expo-doctor",
    "lint": "eslint --cache --fix .",
    "type-check": "tsc --noEmit",
    "start": "doppler run --print-config -- expo start",
    "start-clean": "npm run start -- --clear",
    "dev": "expo start --dev-client",
    "native-android": "expo run:android",
    "native-ios": "expo run:ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-navigation/native": "^6.1.7",
    "@tamagui/animations-react-native": "^1.38.4",
    "@tamagui/babel-plugin": "^1.38.4",
    "@tamagui/config": "^1.38.4",
    "@tamagui/font-inter": "^1.38.4",
    "@tamagui/theme-base": "^1.38.4",
    "@tamagui/toast": "^1.38.4",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "burnt": "^0.11.7",
    "expo": "~49.0.0",
    "expo-dev-client": "~2.4.4",
    "expo-font": "~11.4.0",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.0",
    "expo-splash-screen": "~0.20.4",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-updates": "~0.18.8",
    "expo-web-browser": "~12.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-use-modal-hooks": "^1.3.0",
    "react-native-web": "~0.19.6",
    "tamagui": "^1.38.4"
  }
}
