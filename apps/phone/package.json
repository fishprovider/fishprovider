{
  "name": "@fishprovider/phone",
  "version": "5.0.0",
  "scripts": {
    "doctor": "npx expo-doctor",
    "lint": "eslint --cache --fix .",
    "type-check": "tsc --noEmit",
    "clean-build": "rm -rf .expo",
    "start": "NODE_ENV=development doppler run --print-config -- expo start",
    "dev": "NODE_ENV=development expo start --dev-client",
    "native-android": "expo run:android",
    "native-ios": "expo run:ios",
    "eas-build-pre-install": "./clean-deps.sh",
    "eas-build-post-install": "./build-deps.sh",
    "prebuild-local-ios": "rm -rf *.ipa",
    "build-local-ios": "npx eas-cli@latest build --local --non-interactive --profile production -p ios",
    "prebuild-local-android": "rm -rf *.aab",
    "build-local-android": "npx eas-cli@latest build --local --non-interactive --profile production -p android",
    "build-local": "npm run build-local-ios && npm run build-local-android",
    "submit-local-ios": "BUILD_FILE_NAME=`ls build-*.ipa` && npx eas-cli@latest submit --path=./${BUILD_FILE_NAME} -p ios",
    "submit-local-android": "BUILD_FILE_NAME=`ls build-*.aab` && npx eas-cli@latest submit --path=./${BUILD_FILE_NAME} -p android",
    "submit-local": "npm run submit-local-ios && npm run submit-local-android",
    "build-submit-local": "npm run build-local && npm run submit-local",
    "build-cloud": "npx eas-cli@latest build --non-interactive --no-wait --profile production -p all --auto-submit",
    "deploy-update": "npx eas-cli@latest update --auto --channel production",
    "ci-lint": "eslint --cache --fix",
    "ci-type-check": "tsc-files --noEmit types/global.d.ts"
  },
  "dependencies": {
    "@fishprovider/adapter-frontend": "*",
    "@fishprovider/application": "*",
    "@fishprovider/cross": "*",
    "@fishprovider/enterprise": "*",
    "@fishprovider/framework-fish-api": "*",
    "@fishprovider/framework-local": "*",
    "@fishprovider/framework-offline-first": "*",
    "@fishprovider/framework-store": "*",
    "@fishprovider/utils": "*",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-firebase/app": "^18.3.0",
    "@react-native-firebase/auth": "^18.3.0",
    "@react-native-google-signin/google-signin": "^10.0.1",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@tamagui/babel-plugin": "^1.52.6",
    "@tamagui/config": "^1.52.6",
    "@tamagui/toast": "^1.52.6",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "burnt": "^0.10.0",
    "expo": "~48.0.20",
    "expo-apple-authentication": "~6.0.1",
    "expo-build-properties": "~0.6.0",
    "expo-dev-client": "~2.2.1",
    "expo-device": "~5.2.1",
    "expo-font": "~11.1.1",
    "expo-notifications": "~0.18.1",
    "expo-status-bar": "~1.4.4",
    "expo-system-ui": "~2.2.1",
    "expo-updates": "~0.16.4",
    "expo-web-browser": "~12.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.71.8",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-logs": "^5.0.1",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-web": "~0.18.10",
    "socket.io-client": "^4.7.2",
    "tamagui": "^1.52.6"
  },
  "devDependencies": {
    "@tsconfig/react-native": "^3.0.2"
  }
}