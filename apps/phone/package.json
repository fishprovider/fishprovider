    "@react-native-google-signin/google-signin": "^10.0.1",
    "@react-navigation/native": "^6.1.7",
    "@tamagui/animations-react-native": "^1.39.5",
    "@tamagui/babel-plugin": "^1.39.5",
    "@tamagui/config": "^1.39.5",
    "@tamagui/font-inter": "^1.39.5",
    "@tamagui/theme-base": "^1.39.5",
