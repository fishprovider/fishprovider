{
  "name": "@fishprovider/web",
  "version": "5.0.0",
  "scripts": {
    "lint": "eslint --cache --fix .",
    "type-check": "tsc --noEmit",
    "test": "NODE_ENV=test DOPPLER_PROJECT=${DOPPLER_PROJECT:-web} DOPPLER_CONFIG=${DOPPLER_CONFIG:-dev} doppler run --print-config -- jest",
    "clean-build": "rm -rf .next",
    "prebuild": "./build-deps.sh",
    "build": "NODE_ENV=production DOPPLER_PROJECT=${DOPPLER_PROJECT:-web} DOPPLER_CONFIG=${DOPPLER_CONFIG:-prd} doppler run --print-config -- next build",
    "postbuild": "NODE_ENV=production next-sitemap",
    "build-vercel": "NODE_ENV=production && npm run prebuild && next build && npm run postbuild",
    "start": "NODE_ENV=production DOPPLER_PROJECT=${DOPPLER_PROJECT:-web} DOPPLER_CONFIG=${DOPPLER_CONFIG:-prd} doppler run --print-config -- next start",
    "export": "NODE_ENV=production next export",
    "dev": "NODE_ENV=development DOPPLER_PROJECT=${DOPPLER_PROJECT:-web} DOPPLER_CONFIG=${DOPPLER_CONFIG:-dev} doppler run --print-config -- next dev",
    "analyze": "NODE_ENV=production ANALYZE=true DOPPLER_PROJECT=${DOPPLER_PROJECT:-web} DOPPLER_CONFIG=${DOPPLER_CONFIG:-prd} doppler run --print-config -- next build",
    "ci-lint": "eslint --cache --fix",
    "ci-type-check": "tsc-files --noEmit types/global.d.ts"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fishbot/chart-annotations": "^1.3.1",
    "@fishbot/chart-axes": "^1.3.1",
    "@fishbot/chart-coordinates": "^1.3.1",
    "@fishbot/chart-core": "^1.3.1",
    "@fishbot/chart-indicators": "^1.3.0",
    "@fishbot/chart-interactive": "^1.3.1",
    "@fishbot/chart-scales": "^1.3.1",
    "@fishbot/chart-series": "^1.3.2",
    "@fishbot/chart-tooltip": "^1.3.1",
    "@fishbot/chart-utils": "^1.3.0",
    "@fishprovider/cross": "*",
    "@fishprovider/services": "*",
    "@fishprovider/repository-offline-first": "*",
    "@giscus/react": "^2.3.0",
    "@mantine/core": "^6.0.19",
    "@mantine/dates": "^6.0.19",
    "@mantine/form": "^6.0.19",
    "@mantine/hooks": "^6.0.19",
    "@mantine/modals": "^6.0.19",
    "@mantine/next": "^6.0.19",
    "@mantine/notifications": "^6.0.19",
    "@mantine/nprogress": "^6.0.19",
    "@mantine/spotlight": "^6.0.19",
    "@mui/icons-material": "5.14.6",
    "@mui/material": "5.14.6",
    "@next/bundle-analyzer": "^13.4.19",
    "@sentry/nextjs": "^7.65.0",
    "@sentry/react": "7.65.0",
    "@tinymce/tinymce-react": "^4.3.0",
    "animate.css": "^4.1.1",
    "d3-dsv": "^3.0.1",
    "d3-format": "^3.1.0",
    "d3-time-format": "^4.1.0",
    "dayjs": "^1.11.9",
    "disqus-react": "^1.1.5",
    "firebase": "^10.3.0",
    "html-react-parser": "^4.2.1",
    "i18next": "^23.4.6",
    "i18next-chained-backend": "^4.4.0",
