{
  "name": "@fishbot/web",
  "version": "5.0.3",
  "scripts": {
    "install-packages": "cd ../.. && npm install -w packages/utils -w packages/cross",
    "build-packages": "cd ../.. && npm run build -w packages/utils -w packages/cross",
    "preinstall": "npm run install-packages",
    "lint": "eslint --cache --fix --ignore-pattern public .",
    "type-check": "tsc --noEmit",
    "test": "cross-env NODE_ENV=test doppler run --print-config -- jest",
    "prebuild": "npm run build-packages",
    "build": "cross-env NODE_ENV=production next build",
    "postbuild": "cross-env NODE_ENV=production next-sitemap",
    "start": "cross-env NODE_ENV=production next start",
    "export": "cross-env NODE_ENV=production next export",
    "build-export": "npm run build && npm run export",
    "predev": "npm run build-packages",
    "dev": "cross-env NODE_ENV=development doppler run -p web -c prd_local --print-config -- next dev",
    "analyze": "cross-env NODE_ENV=production ANALYZE=true doppler run --print-config -- next build"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
