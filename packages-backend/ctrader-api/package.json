{
  "name": "@fishprovider/ctrader-api",
  "version": "7.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "postinstall": "npm run build -w ../../packages-share/core-utils -w ../../packages-share/repositories",
    "lint": "eslint --cache --fix .",
    "type-check": "tsc --noEmit",
    "test": "NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules npx jest",
    "clean": "rm -rf dist *.tsbuildinfo && mkdir dist",
    "build": "tsc -p tsconfig.build.json",
    "ci-lint": "eslint --cache --fix",
    "ci-type-check": "tsc-files --noEmit"
  },
  "dependencies": {
    "@fishprovider/core-utils": "*",
    "@fishprovider/repositories": "*",
    "app-root-path": "^3.1.0",
    "protobufjs": "^5.0.3"
  },
  "devDependencies": {
    "@types/protobufjs": "^5.0.31"
  }
}