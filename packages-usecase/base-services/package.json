{
  "name": "@fishprovider/base-services",
  "version": "7.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "postinstall": "npm run build -w ../../packages-share/core-utils && npm run build -w ../../packages-share/repositories",
    "lint": "eslint --cache --fix .",
    "type-check": "tsc --noEmit",
    "test": "NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules npx jest",
    "clean": "rm -rf dist *.tsbuildinfo",
    "build": "tsc -p tsconfig.build.json",
    "ci-lint": "eslint --cache --fix",
    "ci-type-check": "tsc-files --noEmit"
  },
  "dependencies": {
    "@fishprovider/core-utils": "*",
    "@fishprovider/repositories": "*"
  }
}