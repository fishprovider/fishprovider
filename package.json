{
  "name": "fishprovider",
  "version": "6.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "repositories-backend/*",
    "repositories-frontend/*",
    "workers/*",
    "apps/*"
  ],
  "type": "module",
  "engines": {
    "node": ">=16 <19"
  },
  "scripts": {
    "ci": "npm ci --prefer-offline --no-audit --no-progress --legacy-peer-deps",
    "postinstall": "npx husky install",
    "lint": "npm run lint -ws",
    "type-check": "npm run type-check -ws",
    "check-deps": "cd scripts && node check-deps.cjs",
    "build": "./build-deps.sh",
    "doctor": "npm run build && npm run lint && npm run type-check && npm run check-deps",
    "pm2-start": "pm2 startOrReload pm2.config.cjs",
    "pm2-stop": "pm2 stop pm2.config.cjs",
    "pm2-start-only": "npm run pm2-start -- --only",
    "pm2-stop-only": "npm run pm2-stop -- --only",
    "pm2-deploy-setup": "pm2 deploy pm2.config.cjs ${DEPLOY_ENV:=localhost} setup",
    "pm2-deploy": "pm2 deploy pm2.config.cjs ${DEPLOY_ENV:=localhost}",
    "pm2-deploy-ci": "pm2 deploy pm2.config.cjs ${DEPLOY_ENV:=localhost} exec 'source ~/.zshrc && npm run ci'",
    "pm2-deploy-build": "pm2 deploy pm2.config.cjs ${DEPLOY_ENV:=localhost} exec 'source ~/.zshrc && npm run build -w'",
    "pm2-deploy-start": "DOPPLER_TOKEN=$(doppler configure get token --plain) pm2 deploy pm2.config.cjs ${DEPLOY_ENV:=localhost} exec 'source ~/.zshrc && npm run pm2-start-only'",
    "pm2-deploy-stop": "pm2 deploy pm2.config.cjs ${DEPLOY_ENV:=localhost} exec 'source ~/.zshrc && npm run pm2-stop-only'",
    "heroku-build": "npm run build -w workers/pup",
    "heroku-cleanup": "NPM_CONFIG_PRODUCTION=true npm prune --production",
    "heroku-postbuild": "npm run heroku-build && npm run heroku-cleanup"
  },
