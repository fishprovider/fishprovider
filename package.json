{
  "name": "fishprovider",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "workers/*",
    "apps/*"
  ],
  "type": "module",
  "engines": {
    "node": ">=16 <19"
  },
  "scripts": {
    "lint": "npm run lint -ws",
    "type-check-pre": "npm run type-check -w packages/utils",
    "type-check-all": "npm run type-check -ws",
    "type-check": "npm run type-check-pre && npm run type-check-all",
    "doctor": "npm run lint && npm run type-check",
    "heroku-build": "npm run build -w workers/pup",
    "heroku-cleanup": "NPM_CONFIG_PRODUCTION=true npm prune --production",
    "heroku-postbuild": "npm run heroku-build && npm run heroku-cleanup"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@tsconfig/node-lts-strictest-esm": "^18.12.1",
    "@tsconfig/react-native": "^3.0.2",
    "@types/amqplib": "^0.10.1",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/crypto-js": "^4.1.1",
    "@types/d3-format": "^3.0.1",
    "@types/d3-time-format": "^4.0.0",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/long": "^3.0.32",
    "@types/lqip-modern": "^1.1.3",
    "@types/md5": "^2.3.2",
    "@types/node": "20.4.0",
    "@types/node-telegram-bot-api": "^0.61.6",
    "@types/pg": "^8.10.2",
    "@types/protobufjs": "^5.0.31",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.18.11",
    "eslint": "^8.44.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "13.4.8",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^29.6.0",
    "lint-staged": "^13.2.3",
    "nodemon": "^2.0.22",
    "socket.io-client": "^4.7.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsc-files": "^1.1.4",
    "tsc-watch": "^6.0.4",
    "type-fest": "^3.12.0",
    "typescript": "~5.0.4"
  }
}