{
  "name": "fishprovider",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "adapters/*",
    "apps/*",
    "frameworks/*",
    "packages/*",
    "workers/*"
  ],
  "type": "module",
  "engines": {
    "node": ">=16 <19"
  },
  "scripts": {
    "lint": "npm run lint -ws",
    "type-check": "npm run type-check -ws",
    "doctor": "./build-deps.sh && npm run lint && npm run type-check",
    "heroku-build": "npm run build -w workers/pup",
    "heroku-cleanup": "NPM_CONFIG_PRODUCTION=true npm prune --production",
    "heroku-postbuild": "npm run heroku-build && npm run heroku-cleanup"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/d3-format": "^3.0.1",
    "@types/d3-time-format": "^4.0.0",
    "@types/dotenv-flow": "^3.2.0",
    "@types/jest": "^29.5.3",
    "@types/lqip-modern": "^1.1.3",
    "@types/node": "20.4.8",
    "@types/node-telegram-bot-api": "^0.61.7",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "axios": "^1.4.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "13.4.8",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "glob": "^9.3.5",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "lint-staged": "^13.2.3",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-files": "^1.1.4",
    "tsc-watch": "^6.0.4",
    "type-fest": "^4.1.0"
  }
}
