{
  "name": "@fishprovider/utils",
  "version": "5.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint --cache --fix .",
    "type-check": "tsc",
    "test": "npm run test-unit",
    "test-unit": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test-int": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules doppler run --print-config -- npx jest --runInBand",
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json"
  },
  "dependencies": {
    "crypto-random-string": "^5.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@tsconfig/node-lts-strictest-esm": "^18.12.1"
  }
}