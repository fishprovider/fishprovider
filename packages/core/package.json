{
  "name": "@fishbot/core",
  "version": "5.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint --cache --fix --ignore-pattern dist .",
    "type-check": "tsc --noEmit",
    "test": "npm run test-unit",
    "test-unit": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test-int": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules doppler run --print-config -- npx jest --runInBand",
    "build": "rm -rf dist && mkdir dist && cp package.json dist && npm run build-ts",
    "build-ts": "tsc && tsc-alias --verbose"
  },
  "dependencies": {
    "@sentry/node": "^7.56.0",
    "agenda": "^5.0.0",
    "amqplib": "^0.10.3",
    "axios": "^1.4.0",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "discord.js": "^14.11.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.9.0",
    "googleapis": "^118.0.0",
    "lodash": "^4.17.21",
    "loglevel": "^1.8.1",
    "moment": "^2.29.4",
    "mongodb": "^5.6.0",
    "p-queue": "^7.3.4",
    "pg": "^8.11.0",
    "redis": "^4.6.7",
    "socket.io": "^4.7.0",
    "systeminformation": "^5.18.4"
  }
}