{
  "name": "@fishprovider/core",
  "version": "5.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint --cache --fix .",
    "type-check": "tsc --noEmit",
    "test": "npm run test-unit",
    "test-unit": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test-int": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules doppler run --print-config -- npx jest --runInBand",
    "clean-build": "rm -rf dist && mkdir dist",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json"
  },
  "dependencies": {
    "@fishprovider/utils": "*",
    "@sentry/node": "^7.57.0",
    "agenda": "^5.0.0",
    "amqplib": "^0.10.3",
    "axios": "^1.4.0",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "discord.js": "^14.11.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.9.0",
    "googleapis": "^120.0.0",
    "lodash": "^4.17.21",
    "loglevel": "^1.8.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mongodb": "^5.7.0",
    "p-queue": "^7.3.4",
    "pg": "^8.11.1",
    "redis": "^4.6.7",
    "socket.io": "^4.7.1",
    "systeminformation": "^5.18.6"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/compression": "^1.7.2",
    "@types/express-session": "^1.17.7",
    "@types/pg": "^8.10.2",
    "esbuild": "^0.18.11"
  }
}